{
  "name": "metron-management-ui",
  "version": "0.4.3",
  "license": "Apache-2.0",
  "config": {
    "node_ace": "node_modules/ace-builds/src-min-noconflict/"
  },
  "angular-cli": {},
  "scripts": {
    "build": "./node_modules/angular-cli/bin/ng build -prod",
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\" && tslint \"e2e/**/*.ts\"",
    "test": "./node_modules/angular-cli/bin/ng test --watch=false",
    "pree2e": "webdriver-manager update",
    "e2e": "./node_modules/.bin/protractor-flake",
    "e2e-all": " npm run pree2e && ./node_modules/.bin/protractor-flake -- --suite=all",
    "copy-ace": "cp -f $npm_package_config_node_ace/ext-language_tools.js $npm_package_config_node_ace/mode-json.js $npm_package_config_node_ace/theme-monokai.js $npm_package_config_node_ace/worker-json.js src/assets/ace",
    "copy-ace-snippets": "cp -f $npm_package_config_node_ace/snippets/text.js $npm_package_config_node_ace/snippets/json.js  src/assets/ace/snippets",
    "postinstall": "npm run copy-ace & npm run copy-ace-snippets"
  },
  "private": true,
  "dependencies": {
    "@types/ace": "0.0.32",
    "@types/bootstrap": "^3.3.32",
    "@types/jasmine": "2.2.30",
    "@types/jquery": "^2.0.32",
    "@types/tether": "^1.1.27",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "ace-builds": "^1.2.6",
    "bootstrap": "4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "rxjs": "^5.1.0",
    "tether": "^1.3.4",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "angular-cli": "1.0.0-beta.15",
    "buffer-shims": "^1.0.0",
    "codelyzer": "~0.0.26",
    "compression": "1.6.2",
    "copy": "^0.3.0",
    "express": "^4.15.3",
    "http-proxy-middleware": "^0.17.4",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-istanbul": "0.6.0",
    "phantomjs-prebuilt": "^2.1.14",
    "protractor": "5.2.1",
    "protractor-flake": "^2.5.0",
    "serve-static": "^1.12.3",
    "ts-node": "1.2.1",
    "tslint": "3.13.0",
    "typescript": "~2.0.3",
    "@types/request": "2.0.3"
  }
}
