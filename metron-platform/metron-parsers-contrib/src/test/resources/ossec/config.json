{
  "chain": [
    "regex_file_parser",
    "parse_timestamp",
    "rename_fields"
  ],
  "parsers": {
    "regex_file_parser": {
      "class": "org.apache.metron.parsers.contrib.links.io.RegexFileLink",
      "patterns": [
        "^AV\\s+\\-\\sAlert\\s+\\-\\s\\\"(?<timestamp>\\d+)\\\"\\s\\-\\->\\sRID\\:\\s\\\"(?<ruleid>\\d+)\\\"\\;\\s+RL\\:\\s+\\\"(?<rulelevel>\\d+)\\\"\\;\\s+RG\\:\\s+\\\"(?<rulegroup>[^\\\"]*)\\\"\\;\\s+RC\\:\\s+\\\"(?<rulecomment>[^\\\"]*)\\\";.*?HOSTNAME\\:\\s*\"?\\((?<hostname>[^\\)]*)\\)\\s(?<winip>\\S+)->.*?AUDIT_SUCCESS\\((?<wineventid>(4634|4647))\\).*?Account\\s+Name\\:\\s+(?<username>.*?)\\s*Account\\s+Domain\\:\\s+(?<domain>.*?)\\s*Logon\\s+ID\\:\\s+(?<logonid>\\S+)(\\s+Logon\\s+Type\\:\\s+(?<logontype>\\d+))?.*"
      ],
      "input": "{{original_string}}"
    },
    "parse_timestamp": {
      "class": "org.apache.metron.parsers.contrib.links.io.TimestampLink",
      "patterns": [
        [
          "timestamp",
          "newest"
        ],
        [
          "([0-9]{4})-([0-9]{2})-([0-9]{2}) ([0-9]{2}):([0-9]{2}):([0-9]{2})",
          "yyyy MM dd HH mm ss",
          "newest"
        ],
        [
          "([0-9]{4})-([0-9]{2})-([0-9]{2}) ([0-9]{2}):([0-9]{2}):([0-9]{2}) ([+-]{1}[0-9]{1,2}[:][0-9]{2})",
          "yyyy MM dd HH mm ss Z",
          "newest"
        ]
      ],
      "input": "{{original_string}}"
    },
    "rename_fields": {
      "class": "org.apache.metron.parsers.contrib.links.fields.RenameLink",
      "rename": {
        "srcip": "ip_src_addr",
        "srcport": "ip_src_port"
      }
    }
  }
}